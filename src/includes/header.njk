<header id="header">
  <div class="content">
    <div class="inner">
      <h1>{{ site.title }}</h1>
      <p>{{ site.tagline }}</p>
    </div>
  </div>
  <nav>
    <ul>
      {% for item in nav %}
      <li><a href="{{ item.href }}">{{ item.label }}</a></li>
      {% endfor %}
    </ul>
  </nav>
</header>


